#!/usr/bin/env python

deps = [
	'System.dll',
	'System.Data.dll',
	'System.Web.dll',
	'System.Xml.dll',
]

root = bld.path.find_dir('src')

bld(
	features = 'cs',
	use = deps,
	path = root,
	source = root.ant_glob('**/*.cs', excl='AssemblyDescFX1_0.cs'),
	name = 'CookComputing.XmlRpcV2.dll',
	csflags = [ '/nowarn:0649,0618,0219' ],
	version = False,
)

